<Page x:Class="Nameless.InfoPhoenix.Client.Views.Pages.HomePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Nameless.InfoPhoenix.Client.Views.Pages"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      
      d:DataContext="{d:DesignInstance local:HomePage, IsDesignTimeCreatable=False}"
      
      mc:Ignorable="d" 
      d:DesignHeight="450"
      d:DesignWidth="800"
      Title="{Binding ViewModel.Title, Mode=OneWay}">

    <StackPanel Margin="24">
        <ui:TextBlock
            Margin="0,0,0,10"
            FontTypography="Title"
            Text="Home" />
    </StackPanel>
</Page>
